server {
  listen 80;
  root /var/www/grabeh.net/public;
  index index.html;
  server_name mbg.codes www.mbg.codes;
  location / {
    try_files $uri $uri/ =404;
  }
  
  location ~* \.(?:ico|css|js|gif|jpe?g|png)$ {
    expires 30d;
    add_header Pragma public;
    add_header Cache-Control "public";
}
